class Solution {
    public int solution(int a, int b) {
        // a ⊕ b 계산
        int result1 = Integer.parseInt(Integer.toString(a) + Integer.toString(b));

        // b ⊕ a 계산
        int result2 = Integer.parseInt(Integer.toString(b) + Integer.toString(a));

        // 더 큰 값 반환
        return Math.max(result1, result2);
    }
}
