import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        // 이등변 삼각형 출력
        for (int i = 1; i <= n; i++) {
            // 각 행별로 '*' 출력
            for (int j = 1; j <= i; j++) {
                System.out.print("*");
            }
            // 행 변경
            System.out.println();
        }

        // Scanner 객체 닫기
        sc.close();
    }
}
